public class leap{
    public static void main(String[] args) {
        int year =1704;
        if(year %4 == 0 ){
            if(year% 100 != 0 || year% 400 == 0){
                System.out.println("leap");
            }
            else
            System.out.println("not.leap");
        }
        else
        System.out.println("not leap");
    }
}
