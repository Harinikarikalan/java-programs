class prep{
    int roll;
    String name;

    public void display(){
    System.out.println(roll);
    System.out.println(name);
}
    
    public void setvalue(String str,int num){
        name =str;
        roll =num;
    }
}
class Main {
    public static void main(String[] args) {
        prep obj = new prep();
        obj.setvalue("hi",23);
        obj.display();
        
}
}
